{
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "esnext",
		"module": "esnext",
		"isolatedModules": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "./svelte-portfolio",
		"checkJs": true
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
